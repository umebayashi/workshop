@model MVCScheduler.Web.Areas.Master.Models.AppUserListViewModel

@{
    ViewBag.Title = "Index";
}

<h2>ユーザマスタ - 一覧</h2>

<table class="data-grid">
	<tr class="table-header">
		<th></th>
		<th>コード</th>
		<th>名前</th>
	</tr>
	@foreach (var item in Model.Items)
	{
	<tr>
		<td>
            @Html.ActionLink("編集", "Edit", new { id = item.ID })
            ／
            @Html.ActionLink("削除", "Delete", new { id = item.ID })
		</td>
		<td>@item.Code</td>
		<td>@item.Name</td>
	</tr>
	}
	<tr class="table-footer">
		<td colspan="3">@Html.ActionLink("新規追加", "Create")</td>
	</tr>
</table>
